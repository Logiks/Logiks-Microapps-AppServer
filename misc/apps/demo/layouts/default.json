{
  "layoutId": "master-shell",
  "type": "page",
  "platforms": ["web", "mobile", "kiosk"],

  "breakpoints": {
    "mobile": { "maxWidth": 767 },
    "tablet": { "minWidth": 768, "maxWidth": 1023 },
    "desktop": { "minWidth": 1024 },
    "kiosk": { "minWidth": 1280, "orientation": "landscape" }
  },

  "children": [
    {
      "id": "app-header",
      "role": "header",
      "className": "",
      "visibleOn": ["web", "mobile", "kiosk"],
      "slots": [
        { "name": "brand", "permissions": ["public"] },
        { "name": "primary-nav", "permissions": ["user", "admin"] },
        {
          "name": "admin-tools",
          "permissions": ["admin"],
          "featureFlag": ["ADMIN_TOOLS"]
        },
        { "name": "user-profile", "permissions": ["user", "admin"] }
      ]
    },
    {
      "id": "app-sidebar",
      "role": "navigation",
      "visibleOn": ["web", "tablet"],
      "slots": [
        { 
          "name": "main-menu", 
          "permissions": ["user", "admin"], 
          "featureFlags": ["ADMIN_MENU_V2", "EXPERIMENT_ADMIN_NAV"]
        }
      ]
    },
    {
      "id": "app-content",
      "role": "main",
      "visibleOn": ["web", "mobile", "kiosk"],
      "slots": [
        {
          "name": "page-body",
          "permissions": ["public", "user", "admin"],
          "featureFlag": "BILLING_DASHBOARD"
        },
        { "name": "page-actions", "permissions": ["user", "admin"] }
      ]
    },
    {
      "id": "app-footer",
      "role": "footer",
      "visibleOn": ["web"],
      "slots": [
        { "name": "legal", "permissions": ["public"] },
        { "name": "support-links", "permissions": ["user", "admin"] }
      ]
    }
  ],
  "slotBindings": {
    "app-header.brand": "BrandLogo",
    "app-header.primary-nav": {
      "component": "UserMenu",
      "params": {

      }
    },
    "app-sidebar.main-menu": {
      "component": "navigator",
      "params": {
        "ui_template": "list"
      },
      "source": {
        "type": "api",
        "method": "get",
        "endpoint": "navigator/app"
      },
      "source1": {
          "type": "sql",
          "table": "docs_tbl,profiletbl",
          "cols": "docs_tbl.id,docs_tbl.doctype,docs_tbl.doclink,docs_tbl.title,docs_tbl.docname,docs_tbl.category,(concat(docs_tbl.docyear,'-',docs_tbl.docyear+1)) as docyear,docs_tbl.issuedOn,docs_tbl.placeOfIssue,docs_tbl.expires,docs_tbl.verfied_on,docs_tbl.flags,profiletbl.full_name,docs_tbl.verfied_by,docs_tbl.verfied_on,docs_tbl.blocked,docs_tbl.created_by,docs_tbl.created_on",
          "where": {
            "docs_tbl.profile_id=profiletbl.id":"RAW",
            "(DATE(docs_tbl.expires)< CURDATE())":"RAW"
            
          },
          "limit": 10
        }
    },
    "app-content.page-body": {
      "remote_component": "component/test1.jsx",
      "params": {
        
      }
    }
  }
}