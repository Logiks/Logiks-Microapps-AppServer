{
    "schema":"1.0",
	"title":"ApiKeys Report",
	"category":"Master",
	"privilege":"*",
	"blocked":false,
	"rowlink":false,
	"rowsPerPage":20,
	"showExtraColumn":false,
	"custombar":false,
		"topbar":{
	    "uitype":"type1"
	},
	"hooks": {
		"fieldupdate": {
		    "modules":["bizflow","bizlogger"]
		}
	},
	"source":{
	    "type":"sql",
	    "table":"auth_apikeys",
	    "where":{
	        "auth_apikeys.blocked":"false",
	      "(auth_apikeys.guid = '#SESS_GUID#'  OR  #SESS_PRIVILEGE_ID# <= #ADMIN_PRIVILEGE_RANGE#)": "RAW"		
	    },
	    "cols":"*",
	    "limit":20
	},
	"actions":{
        "generateApiKeys":{
            "label":"Generate API Key",
            "icon": "fa fa-plus",
    		"class": "btn btn-info"
        }
   },
	"buttons": {
		"infoview@apikeys/{hashid}": {
            "label":"View API Key Deails",
			"icon": "fa fa-eye"
		},
		"forms@apikeys/edit/{hashid}": {
            "label":"Edit API Keys",
			"icon": "fa fa-pencil"
		},
		"infoview@apiKeyInfo/{hashid}": {
            "label":"View API Key",
			"icon": "fa fa-key"
		}
	},
	"datagrid":{
	    "id":{
			"label":"ID",
			"hidden":false,
			"searchable":true,
			"sortable":true,
			"groupable":false,
			"classes":"",
			"style":"width:50px%3B",
			"formatter":"text"
		},
		"title": {
			"label": "Title",
			"hidden":false,
			"searchable":true,
			"sortable":true
		},
		"environment": {
			"label": "Environment",
			"hidden":false,
			"searchable":true,
			"sortable":true,
			"filter":{
			    "type":"dataSelector",
			     "nofilter": "--",
			    "groupid":"appstatus"
			}
		},
		"active_time_start": {
			"label": "Active Time Start",
			"hidden":false,
			"searchable":true,
			"sortable":true
		},

		"scope": {
			"label": "Scope",
			"hidden":false,
			"searchable":true,
			"sortable":true
		},
		"remarks": {
			"label": "Remark",
			"hidden":false,
			"searchable":true,
			"sortable":true,
			"width":12
		},
		"created_by":{
		    "label":"Added By",
		    "sortable": true,
		    "searchable": true,
		    "hidden":true
		},
		"created_on":{
		    "label":"Added On",
		    "sortable": true,
		    "searchable": true,
		    "formatter":"datetime",
			"filter":{
				"type":"date"
			},
		    "hidden":true
		},
		"edited_by": {
			"label": "Updated By",
			"sortable": true,
			"searchable": true,
			"hidden": true
		},
		"edited_on": {
			"label": "Updated On",
			"sortable": true,
			"searchable": true,
			"formatter": "datetime",
			"filter": {
				"type": "date"
			},
			"hidden": true
		}
	}
}